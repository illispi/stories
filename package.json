{
  "name": "solid-stories",
  "scripts": {
    "dev": "solid-start dev --host",
    "build": "solid-start build && tsc ./src/utils/migrate.ts --target esnext --moduleResolution nodenext --module nodenext && mv ./src/utils/migrate.js ./dist/",
    "start": "solid-start start",
    "prod": "solid-start build && solid-start start",
    "start:dev": "solid-start start --host",
    "lint": "eslint --fix \"**/*.{ts,tsx,js,jsx}\"",
    "migrate:latest": "tsx ./src/utils/migrate.ts",
    "migrate:down": "tsx ./src/utils/migrate.ts down",
    "new:migration": "tsx ./src/utils/migrationFilename.ts",
    "routes": "pnpm routes-gen -d @routes-gen/solid-start",
    "sqlTypes": "pnpm kysely-codegen --dialect postgres"
  },
  "type": "module",
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@types/node": "20.10.4",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@vitejs/plugin-basic-ssl": "^1.0.2",
    "autoprefixer": "^10.4.16",
    "dotenv": "^16.3.1",
    "eslint": "^8.55.0",
    "eslint-plugin-solid": "^0.13.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "jest": "^29.7.0",
    "kysely-codegen": "^0.11.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "rollup-plugin-visualizer": "^5.10.0",
    "sass": "^1.69.5",
    "solid-devtools": "^0.29.2",
    "solid-start-node": "^0.3.10",
    "tailwindcss": "^3.3.6",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "ts-to-zod": "^3.5.0",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "vite": "^4.5.0",
    "vite-imagetools": "^6.2.7"
  },
  "dependencies": {
    "@lucia-auth/adapter-postgresql": "^2.0.2",
    "@lucia-auth/oauth": "^3.5.0",
    "@modular-forms/solid": "^0.20.0",
    "@routes-gen/solid-start": "^0.1.1",
    "@sentry/browser": "^7.86.0",
    "@solid-mediakit/trpc": "^1.0.2",
    "@solid-primitives/refs": "^1.0.5",
    "@solid-primitives/script-loader": "^2.0.2",
    "@solid-primitives/transition-group": "^1.0.3",
    "@solidjs/meta": "^0.29.1",
    "@solidjs/router": "^0.8.3",
    "@tanstack/solid-query": "5.12.1",
    "@trpc/client": "^10.44.1",
    "@trpc/server": "^10.44.1",
    "chartist": "^1.3.0",
    "kysely": "^0.26.3",
    "lucia": "^2.7.4",
    "pg": "^8.11.3",
    "routes-gen": "^0.6.1",
    "solid-js": "^1.8.7",
    "solid-start": "^0.3.10",
    "solid-start-trpc": "^0.0.16",
    "solid-transition-group": "^0.2.3",
    "tailwind-merge": "^2.1.0",
    "valibot": "^0.22.0",
    "zod": "^3.22.4"
  },
  "engines": {
    "node": ">=20"
  },
  "jest": {
    "preset": "@next-auth/adapter-test/jest"
  }
}
